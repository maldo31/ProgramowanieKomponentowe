<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.comp.PrimaryController">
   <ComboBox fx:id="comboBoxSystemDifficult" layoutX="238.0" layoutY="59.0" prefWidth="150.0" promptText="Choice">
      <items>
         <FXCollections fx:factory="observableArrayList">
            <String fx:value="UltraEasy"/>
            <String fx:value="Easy"/>
            <String fx:value="Medium"/>
            <String fx:value="Hard"/>
         </FXCollections>
      </items>
   </ComboBox>
   <Button fx:id="buttonConfirmLevel" layoutX="262.0" layoutY="199.0" mnemonicParsing="false"
           onAction="#onActionButtonConfirmLevel" text="Confirm level">
      <font>
         <Font size="14.0"/>
      </font>
   </Button>
   <!-- RIGHT SIDE -->
   <Button fx:id="buttonStartGame" layoutX="464.0" layoutY="72.0" mnemonicParsing="false"
           onAction="#onActionButtonStartGame" prefHeight="117.0" prefWidth="136.0" text="Start Game">
      <font>
         <Font size="20.0"/>
      </font>
   </Button>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
